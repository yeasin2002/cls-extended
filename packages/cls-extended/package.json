{
  "name": "cls-extended",
  "type": "module",
  "version": "1.0.0",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./adapters/vite": {
      "import": "./dist/adapters/vite.js",
      "require": "./dist/adapters/vite.cjs"
    },
    "./adapters/webpack": {
      "import": "./dist/adapters/webpack.js",
      "require": "./dist/adapters/webpack.cjs"
    },
    "./api": {
      "import": "./dist/api.js",
      "require": "./dist/api.cjs"
    },
    "./core/options": {
      "import": "./dist/core/options.js",
      "require": "./dist/core/options.cjs"
    },
    "./core/parser": {
      "import": "./dist/core/parser.js",
      "require": "./dist/core/parser.cjs"
    },
    "./core/transform": {
      "import": "./dist/core/transform.js",
      "require": "./dist/core/transform.cjs"
    },
    "./unplugin-factory": {
      "import": "./dist/unplugin-factory.js",
      "require": "./dist/unplugin-factory.cjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yeasin2002/cls-extended"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "release": "release-it"
  },
  "dependencies": {
    "@babel/parser": "^7.29.0",
    "magic-string": "^0.30.21",
    "unplugin": "^3.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.29.0",
    "@eslint/js": "catalog:",
    "@release-it/conventional-changelog": "^8.0.2",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "auto-changelog": "^2.5.0",
    "bumpp": "^10.4.0",
    "eslint": "catalog:",
    "globals": "catalog:",
    "jiti": "^2.6.1",
    "prettier": "^3.8.1",
    "release-it": "^17.10.0",
    "tsdown": "^0.20.1",
    "tsdown-preset-sxzz": "^0.3.1",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rspack",
    "rollup",
    "rolldown",
    "esbuild",
    "farm"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts"
}